{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017", "dom"],
    "moduleResolution": "node"
  },
  "include": ["src/**/*", "*.ts", "*.tsx"]
}
